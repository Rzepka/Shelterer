            <Source Path="DBContext" DbContext="Shelterer.Models.SheltersContext, Shelterer" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=(LocalDb)\v11.00;AttachDbFilename=C:\www\Shelterer\DB\aspnet-Shelterer.mdf;Initial Catalog=aspnet-Shelterer;Integrated Security=True</ParameterValue>
